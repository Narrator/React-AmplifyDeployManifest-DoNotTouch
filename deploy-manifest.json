{
  "version": 1,
  "framework": { "name": "react", "version": "18.2.0" },
  "imageSettings": {
    "sizes": [100, 200],
    "domains": ["images.unsplash.com"],
    "remotePatterns": [],
    "formats": ["image/jpeg", "image/png", "image/webp", "image/avif"],
    "minimumCacheTTL": 60,
    "dangerouslyAllowSVG": false
  },
  "routes": [
    {
      "path": "/backend/*",
      "target": {
        "kind": "Compute",
        "src": "default"
      }
    },
    {
      "path": "/ahio",
      "target": {
        "kind": "ImageOptimization"
      }
    },
    {
      "path": "/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=0"
      },
      "fallback": {
        "kind": "Compute",
        "src": "default"
      }
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs18.x",
      "entrypoint": "index.cjs"
    }
  ]
}
